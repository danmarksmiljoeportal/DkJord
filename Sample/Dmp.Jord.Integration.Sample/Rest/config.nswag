: true\r\n          },\r\n          \"legalFrameworkCode\": {\r\n            \"maxLength\": 10,\r\n            \"minLength\": 0,\r\n            \"type\": \"string\",\r\n            \"nullable\": true\r\n          },\r\n          \"currentTargetAreaCodes\": {\r\n            \"type\": \"array\",\r\n            \"items\": {\r\n              \"type\": \"string\"\r\n            },\r\n            \"nullable\": true\r\n          },\r\n          \"projectPhaseCode\": {\r\n            \"maxLength\": 10,\r\n            \"minLength\": 0,\r\n            \"type\": \"string\",\r\n            \"nullable\": true\r\n          },\r\n          \"geometries\": {\r\n            \"type\": \"array\",\r\n            \"items\": {\r\n              \"$ref\": \"#/components/schemas/Dmp.Jord.Application.Locations.Contracts.Geometry\"\r\n            },\r\n            \"nullable\": true\r\n          },\r\n          \"pollutants\": {\r\n            \"type\": \"array\",\r\n            \"items\": {\r\n              \"$ref\": \"#/components/schemas/Dmp.Jord.Application.Locations.Contracts.Pollutant\"\r\n            },\r\n            \"nullable\": true\r\n          },\r\n          \"treatments\": {\r\n            \"type\": \"array\",\r\n            \"items\": {\r\n              \"$ref\": \"#/components/schemas/Dmp.Jord.Application.Locations.Contracts.Treatment\"\r\n            },\r\n            \"nullable\": true\r\n          }\r\n        },\r\n        \"additionalProperties\": false\r\n      },\r\n      \"Dmp.Jord.Application.Locations.Contracts.Project\": {\r\n        \"type\": \"object\",\r\n        \"properties\": {\r\n          \"id\": {\r\n            \"type\": \"string\",\r\n            \"format\": \"uuid\"\r\n          },\r\n          \"projectNumberIdentifier\": {\r\n            \"type\": \"integer\",\r\n            \"format\": \"int32\",\r\n            \"nullable\": true\r\n          },\r\n          \"projectDescriptionText\": {\r\n            \"type\": \"string\",\r\n            \"nullable\