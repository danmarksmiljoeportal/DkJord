,\r\n            \"in\": \"query\",\r\n            \"description\": \"Distinct Reference for a Location. Typical format e.g. {745-00084}. A Location is a Jord-specific term used to refer to a specific area within a Region. Search value must be exactly the same as data value for object to be returned.\",\r\n            \"schema\": {\r\n              \"type\": \"string\",\r\n              \"description\": \"Distinct Reference for a Location. Typical format e.g. {745-00084}. A Location is a Jord-specific term used to refer to a specific area within a Region. Search value must be exactly the same as data value for object to be returned.\",\r\n              \"nullable\": true\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"locationName\",\r\n            \"in\": \"query\",\r\n            \"description\": \"Userfriendly Name for a Location. Typical format e.g. {Kobbersmedje i Rebild}. A Location is a Jord-specific term used to refer to a specific area within a Region. Search value must be exactly the same as data value for object to be returned.\",\r\n            \"schema\": {\r\n              \"type\": \"string\",\r\n              \"description\": \"Userfriendly Name for a Location. Typical format e.g. {Kobbersmedje i Rebild}. A Location is a Jord-specific term used to refer to a specific area within a Region. Search value must be exactly the same as data value for object to be returned.\",\r\n              \"nullable\": true\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"locationDescriptionText\",\r\n            \"in\": \"query\",\r\n            \"description\": \"A detailed userfriendly name for a Location. Typical format e.g. {Ældre Bruntvej 1, 8660 Skanderborg}. A Location is a Jord-specific term used to refer to a specific area within a Region. Search value must be exactly the same as data value for object to be returned.\",\r\n            \"schema\": {\r\n              \"type\": \"string\",\r\n              \"description\": \"A detailed userfriendly name for a Location. Typical format e.g. {Ældre Bruntvej 1, 8660 Skanderborg}. A Location is a Jord-specific term used to refer to a specific area within a Region. Search value must be exactly the same as data value for object to be returned.\",\r\n              \"nullable\": true\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"readyForReportingIndicator\",\r\n            \"in\": \"query\",\r\n            \"description\": \"When set to TRUE, it will only return locations that are ready for reporting. When set to FALSE, it will only return locations that are not ready for reporting. Do not send any value in order to ignore this filter.\",\r\n            \"schema\": {\r\n              \"type\": \"boolean\",\r\n              \"description\": \"When set to TRUE, it will only return locations that are ready for reporting. When set to FALSE, it will only return locations that are not ready for reporting. Do not send any value in order to ignore this filter.\",\r\n              \"nullable\": true\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"useCode\",\r\n            \"in\": \"query\",\r\n            \"description\": \"Searches by int (as string) values referring to what the location is being used for. E.g. '17' refers to \\\"Fyld- og losseplads\\\".\",\r\n            \"schema\": {\r\n              \"type\": \"string\",\r\n              \"description\": \"Searches by int (as string) values referring to what the location is being used for. E.g. '17' refers to \\\"Fyld- og losseplads\\\".\",\r\n              \"nullable\": true\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"plannedActionCode\",\r\n            \"in\": \"query\",\r\n            \"description\": \"Searches by  (as string) values referring to actions planned on the location. E.g. '05' refers to \\\"Undersøgelse, videregående\\\".\",\r\n            \"schema\": {\r\n              \"type\": \"string\",\r\n              \"description\": \"Searches by  (as string) values referring to actions planned on the location. E.g. '05' refers to \\\"Undersøgelse, videregående\\\".\",\r\n              \"nullable\": true\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"plannedActionEndDateTime\",\r\n            \"in\": \"query\",\r\n            \"description\": \"Searches for locations which has planned actions that exactly end on same date as the set filter date.\",\r\n            \"schema\": {\r\n              \"type\": \"string\",\r\n              \"description\": \"Searches for locations which has planned actions that exactly end on same date as the set filter date.\",\r\n              \"format\": \"date-time\",\r\n              \"nullable\": true\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"currentTargetAreaCode\",\r\n            \"in\": \"query\",\r\n            \"description\": \"Searches for locations which has planned actions that exactly end on same date as the set filter date.\",\r\n            \"schema\": {\r\n              \"type\": \"string\",\r\n              \"description\": \"Searches for locations which has planned actions that exactly end on same date as the set filter date.\",\r\n              \"nullable\": true\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"isIncludeParcel\",\r\n            \"in\": \"query\",\r\n            \"description\": \"When set to TRUE, each returned Locations will include all Parcel objects that have been affected by pollution areas (Status Declarations) within it.\",\r\n            \"schema\": {\r\n              \"type\": \"boolean\",\r\n              \"description\": \"When set to TRUE, each returned Locations will include all Parcel objects that have been affected by pollution areas (Status Declarations) within it.\"\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"searchText\",\r\n            \"in\": \"query\",\r\n            \"description\": \"The search string of this field will be compared with the fields location reference, location name and location description to see if any of them match.\",\r\n            \"schema\": {\r\n              \"type\": \"string\",\r\n              \"description\": \"The search string of this field will be compared with the fields location reference, location name and location description to see if any of them match.\",\r\n              \"nullable\": true\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"skip\",\r\n            \"in\": \"query\",\r\n            \"description\": \"Results are organized in an indexed order.Using the Skip function, the user can decide from which index to start the search.This parameter combined with the parameter 'take' enables use of paging.\",\r\n            \"schema\": {\r\n              \"type\": \"integer\",\r\n              \"description\": \"Results are organized in an indexed order.Using the Skip function, the user can decide from which index to start the search.This parameter combined with the parameter 'take' enables use of paging.\",\r\n              \"format\": \"int32\",\r\n              \"nullable\": true\r\n            }\r\n          },\r\n          {\r\n            \"name\": \"take\",\r\n            \"in\": \"query\",\r\n            \"description\": \"Species the number of (max) records to return. This parameter combined with the parameter 'skip' enables use of paging.\",\r\n            \"schema\": {\r\n              \"type\": \"integer\",\r\n              \"description\": \"Species the number of (max) records to return. This parameter combined with the parameter 'skip' enables use of paging.\",\r\n              \"format\": \"int32\",\r\n              \"nullable\": true\r\n            }\r\n          }\r\n        ],\r\n        \"responses\": {\r\n          \"200\": {\r\n            \"description\": \"Success\",\r\n            \"content\": {\r\n              \"text/plain\": {\r\n                \"schema\": { }\r\n              },\r\n              \"application/json\": {\r\n                \"schema\": { }\r\n              },\r\n              \"text/json\": {\r\n                \"schema\": { }\r\n              }\r\n            }\r\n          },\r\n          \"401\": {\r\n            \"description\": \"Unauthorized\"\r\n          },\r\n          \"403\": {\r\n            \"description\": \"Forbidden\"\r\n          },\r\n          \"404\": {\r\n            \"description\": \"Not Found\"\r\n          }\r\n        }\r\n      },\r\n      \"post\": {\r\n        \"tags\": [\r\n          \"Location\"\r\n        ],\r\n        \"summary\": \"Creates a new location including information like planned actions, pollution causes, projects, status declarations and more.\",\r\n        \"operationId\": \"CreateLocation\",\r\n        \"requestBody\": {\r\n          \"content\": {\r\n            \"application/json-patch+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/Dmp.Jord.Application.Locations.Contracts.Commands.CreateLocationCommand\"\r\n              }\r\n            },\r\n            \"application/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/Dmp.Jord.Application.Locations.Contracts.Commands.CreateLocationCommand\"\r\n              }\r\n            },\r\n            \"text/json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/Dmp.Jord.Application.Locations.Contracts.Commands.CreateLocationCommand\"\r\n              }\r\n            },\r\n            \"application/*+json\": {\r\n              \"schema\": {\r\n                \"$ref\": \"#/components/schemas/Dmp.Jord.Application.Locations.Contracts.Commands.CreateLocationCommand\"\r\n              }\r\n            }\r\n          }\r\n        },\r\n        \"responses\": {\r\n          \"200\": {\r\n            \"description\": \"Success\",\r\n            \"content\": {\r\n